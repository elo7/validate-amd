{
  "name": "elo7-validate-amd",
  "version": "1.0.1",
  "description": "Small library to help form validation",
  "keywords": [
    "js",
    "form",
    "validate"
  ],
  "scripts": {
    "lint": "eslint validate.js test/*",
    "pretest": "npm run lint && npm run build",
